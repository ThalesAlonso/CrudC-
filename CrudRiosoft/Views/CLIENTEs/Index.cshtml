@model IEnumerable<CrudRiosoft.Models.CLIENTE>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<link rel="stylesheet" href="~/Content/Site.css" />
<p>
    @Html.ActionLink("Adicionando Clientes", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NOME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CPF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DATANASCIMENTO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GENERO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TELEFONE.DDD)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NOME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CPF)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DATANASCIMENTO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GENERO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TELEFONE.DDD)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CLI_COD }) |
            @Html.ActionLink("Details", "Details", new { id=item.CLI_COD }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CLI_COD })
        </td>
    </tr>
}

</table>
